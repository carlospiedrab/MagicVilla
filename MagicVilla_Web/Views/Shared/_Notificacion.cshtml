
<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>

@if(TempData["exitoso"] !=null)
{
   <script>
           Swal.fire({
              position: 'top-end',
              icon: 'success',
              title: '@TempData["exitoso"]',
              showConfirmButton: false,
              timer: 3000
            });

   </script>
}

@if (TempData["error"] != null)
{
    <script>
        Swal.fire({
            position: 'top-end',
            icon: 'error',
            title: '@TempData["error"]',
            showConfirmButton: false,
            timer: 3000
        });

    </script>
}
